{
  "name": "typebox",
  "description": "Universal & Open Source keystroke launcher",
  "version": "0.60.28",
  "license": "MIT",
  "keywords": [
    "desktop",
    "electron",
    "launcher",
    "keystroke",
    "productivity",
    "app"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/karacas/typebox"
  },
  "main": "app/main.js",
  "scripts": {
    "_preinstall": "npm i electron-forge -g",
    "start": "electron-forge start",
    "startProd": "electron-forge start . -- --prod",
    "test": "node ./node_modules/mocha/bin/mocha ./_test/ --timeout 20000",
    "clean_dep": "node ./node_modules/rimraf/bin.js ./package-lock.json && node ./node_modules/rimraf/bin.js ./node_modules",
    "reinstall": "npm run clean_dep && npm i",
    "prettierApp": "node node_modules/prettier/bin/prettier.js --single-quote --print-width 160 --tab-width 4 --write ./app/**/*.{jsx,js}",
    "prettierPacks": "node node_modules/prettier/bin/prettier.js --single-quote --print-width 160 --tab-width 4 --write ./_data/packages_dev/*/*.{jsx,js}",
    "prettierTest": "node node_modules/prettier/bin/prettier.js --single-quote --print-width 160 --tab-width 4 --write ./_test/*.{jsx,js}",
    "prettier": "npm run prettierApp && npm run prettierPacks && npm run prettierTest",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "_packagew64": "electron-packager . --asar=true --platform=win32 --arch=x64 --prune=true --out=out --electron-version=1.6.7",
    "_packagew64NoAsar": "electron-packager . --platform=win32 --arch=x64 --prune=true --out=out --electron-version=1.6.7",
    "_buildWin": "electron-installer-windows --src out/typebox-win32-x64/ --dest dist/installers/"
  },
  "author": {
    "name": "karacas",
    "email": "alejandroe@gmail.com"
  },
  "build": {
    "productName": "typebox",
    "appId": "com.karacas.typebox",
    "directories": {
      "output": "_out/installers"
    }
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm",
          "flatpak"
        ]
      },
      "__electronWinstallerConfig": {
        "iconUrl": "app/assets/icons/color_256.ico",
        "loadingGif": "./assets/img/install-splash-anim.gif"
      },
      "electronWinstallerConfig": {
        "title": "typebox",
        "name": "typebox",
        "description": "typebox launcher",
        "setupIcon": "app/assets/icons/color_256.ico",
        "setupExe": "typebox_setup.exe"
      },
      "electronPackagerConfig": {
        "icon": "app/assets/icons/color_256",
        "asar": true,
        "prune": true,
        "overwrite": true,
        "name": "typebox",
        "appCopyright": "typebox",
        "win32metadata": {
          "ProductName": "typebox",
          "InternalName": "typebox",
          "FileDescription": "typebox launcher",
          "CompanyName": "typebox",
          "OriginalFilename": "typebox.exe"
        },
        "ignore": [
          "/_data",
          "/_dev",
          "/_choco",
          "/_test",
          "/app/colors/palette.styl",
          "/app/css/style.styl",
          "\\.css\\.map",
          "\\.js\\.map",
          "\\.min\\.map",
          "\\.wiki",
          "/node_modules/mathjs/dist/math.map",
          "/node_modules/robotjs/build/Release/robotjs.map",
          "/node_modules/axios/dist/axios.map",
          "/node_modules/npm/test",
          "/node_modules/npm/changelogs",
          "/node_modules/mathjs/test",
          "/node_modules/mousetrap/tests",
          "/node_modules/npm/html/doc",
          "/node_modules/mathjs/docs",
          "/node_modules/mathjs/examples",
          "/node_modules/tinycolor2/demo/",
          "/node_modules/tinycolor2/test/",
          "/node_modules/tinycolor2/docs/",
          "/node_modules/decimal.js/docs/",
          "README\\.markdown",
          "yarn\\.lock",
          "CHANGELOG\\.md",
          "CONTRIBUTING\\.md",
          "editorconfig",
          "History\\.md",
          "README\\.md"
        ]
      },
      "electronInstallerDMG": {
        "icon": "app/assets/icons/color_256.icns"
      },
      "electronInstallerDebian": {
        "icon": "app/assets/icons/color_256.png",
        "categories": [
          "productivity"
        ],
        "homepage": "https://github.com/karacas/typebox"
      },
      "github_repository": {
        "owner": "karacas",
        "name": "typebox"
      }
    }
  },
  "dev__": {
    "_npmi": "2.0.1",
    "_sntp": "2.1.0",
    "_electron": "^1.7.9",
    "_electron-prebuilt-compile": "1.7.9"
  },
  "dependencies": {
    "all": "^0.0.0",
    "app2png": "^1.0.0",
    "babel-core": "^6.26.0",
    "babel-plugin-inferno": "^3.3.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "crypto-js": "^3.1.9-1",
    "deep-diff": "^0.3.8",
    "diacritics": "^1.3.0",
    "electron-compile": "^6.4.2",
    "electron-is-accelerator": "^0.1.2",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.12",
    "electron-shortcut-normalizer": "^1.0.0",
    "file-extension": "^4.0.1",
    "fs-extra": "^5.0.0",
    "fs-plus": "^3.0.1",
    "get-package-readme": "^1.2.0",
    "get-repository-url": "^0.1.1",
    "globby": "^7.1.1",
    "highlight.js": "^9.12.0",
    "immutable": "^4.0.0-rc.9",
    "inferno": "^3.10.1",
    "inferno-component": "^3.10.1",
    "inferno-create-class": "^3.10.1",
    "inferno-create-element": "^3.10.1",
    "is-elevated": "^2.0.1",
    "is-url": "^1.2.2",
    "isexe": "^2.0.0",
    "js-yaml": "^3.10.0",
    "json5": "^0.5.1",
    "lodash": "^4.17.4",
    "marked": "^0.3.7",
    "mathjs": "^3.17.0",
    "mkpath": "^1.0.0",
    "moment": "^2.19.4",
    "mousetrap": "^1.6.1",
    "msgpack-lite": "^0.1.26",
    "node-watch": "^0.5.5",
    "npmi": "2.0.1",
    "object-hash": "^1.2.0",
    "opn": "^5.1.0",
    "path-exists": "^3.0.0",
    "q": "^1.5.1",
    "redux": "^3.7.2",
    "redux-observers": "^2.0.2",
    "rimraf": "^2.6.2",
    "sanitize-html": "^1.16.1",
    "semver": "^5.4.1",
    "sntp": "2.1.0",
    "superagent": "^3.8.2",
    "superagent-cache": "^2.0.0",
    "superagent-no-cache": "^0.1.1",
    "svelte": "^1.47.1",
    "syntax-error": "^1.3.0",
    "threads": "^0.10.0",
    "tinycolor2": "^1.4.1",
    "urlsafe-base64": "^1.0.0",
    "user-home": "^2.0.0",
    "walk": "^2.3.9"
  },
  "optionalDependencies": {
    "robotjs": "^0.4.7"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "electron": "1.7.9",
    "electron-prebuilt-compile": "1.7.9",
    "mocha": "^4.0.1",
    "prettier": "^1.9.1",
    "spectron": "^3.7.2"
  }
}
