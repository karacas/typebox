{
   "name": "typebox",
   "version": "0.86.444",
   "description": "Universal & Open Source keystroke launcher",
   "license": "MIT",
   "engines": {
      "node": ">= 10",
      "npm": ">= 6",
      "yarn": ">= 1.13"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/karacas/typebox"
   },
   "author": "karacas <alejandroe@gmail.com>",
   "main": "app/main.js",
   "config": {
      "forge": "./forge.config.js"
   },
   "_moduleAliases": {
      "@root": "./app/",
      "@api": "./app/api/",
      "@main": "./app/main/",
      "@aux": "./app/auxs/",
      "@render": "./app/render/",
      "@mainview": "./app/main_view",
      "@components": "./app/components/",
      "@packages_internal": "./app/packages_internal/",
      "@uitests": "./app/uitests/",
      "@captures": "./app/captures/",
      "@css": "./app/css/",
      "@assets": "./app/assets/"
   },
   "scripts": {
      "__preinstall": "npm i rimraf -g",
      "__preinstall:windows": "npm i rimraf -g && npm i windows-build-tools -g",
      "autoGit": "node scripts.js autoGit && git push",
      "autoGitValid": "npm run taskr:all && npm run test:all && npm run autoGit",
      "build": "npm run make",
      "bump": "npm-check-updates -u -x electron,npm-check-updates",
      "callProd": "%APPDATA%/../Local/typebox/typebox.exe -- --startOpen=false",
      "capture": "npx electron . --user_settings_file=_data_capture/user/user_settings.json --chromiumConsole=false --prod",
      "cleanDep": "(rimraf ./node_modules || true) && (rimraf ./out || true) && (rimraf ./node_modules_bak || true)",
      "cost:global": "cost-of-modules --yarn --no-install",
      "unused:global": "npm-check && depcheck",
      "debloat": "(npx taskr pruneDev || true)",
      "debloat:test": "(npx taskr pruneTest || true)",
      "debugProd": "%APPDATA%/../Local/typebox/typebox.exe -- --verbose --prod",
      "electronrebuild": "npx electron-rebuild",
      "forceUpdate": "(npm run cleanDep || true) && yarn && npm run packagesDevInstall && npm run debloat && npm run electronrebuild && npm run symPacks",
      "forceUpdate:npm": "(npm run cleanDep || true) && (npm i || true) && npm run debloat && (npm run electronrebuild || true)",
      "forceUpdate:safe": "(npm run cleanDep || true) && yarn && npm run packagesDevInstall && npm run electronrebuild",
      "forceUpdate:npmsafe": "(npm run cleanDep || true) && (npm i || true) && npm run electronrebuild",
      "gitMergeDev": "git branch -C test dev && git push origin dev",
      "gitMergeProd": "git branch -C stable master && git push origin master",
      "gitMergeStable": "git branch -C dev stable && git push origin stable",
      "invisible": "npx electron . --invisible=true --chromiumConsole=false",
      "lint": "eslint .eslintrc c:/typebox/app/**/*.js --fix",
      "lint:jsx": "eslint .eslintrc c:/typebox/app/**/*.jsx --fix",
      "lint:all": "(npm run lint || true) &&  npm run lint:jsx",
      "make": "npm run test:all && npm run makeBabel && npm run make:css && npm run format && npm run optimize && (rimraf ./out || true) && electron-forge make",
      "make:notest": "npm run makeBabel && npm run make:css && npm run format:fast && npm run optimize && (rimraf ./out || true) && electron-forge make",
      "packagesDevInstall": "(taskr packagesYarn || true)",
      "symPacks": "(taskr symPacks || true)",
      "makeBabel": "taskr makeBabel",
      "make:css": "taskr makeStylus",
      "make:fast": "(npm run makeBabel || true) && (rimraf ./out || true) && electron-forge make",
      "ncu2": "ncu -x npm-check-updates,electron",
      "optimize": "(npm run optimize:css || true) && (npm run uglify || true)",
      "optimize:css": "taskr optimizeCss",
      "prettier": "taskr prettierAll",
      "format": "taskr prettierAll",
      "format:fast": "taskr prettierFast",
      "rebuild": "npm run forceUpdate && npm run make",
      "rebuild:npm": "(npm run forceUpdate:npm || true) && npm run make",
      "rebuild:safe": "npm run forceUpdate:safe && npm run make",
      "republish": "npm run bump && npm run rebuild",
      "start": "electron-forge start",
      "start:prod": "electron-forge start . -- --prod",
      "start:keys": "npx electron . --debug.canListenKeyboard=1",
      "taskr:all": "taskr",
      "test": "npm run test:all",
      "test:all": "npm run format:fast && npm run test:ui",
      "test:fast": "npm run test:ui",
      "test:debug": "npx electron . --user_settings_file=_data_test/user/user_settings.json --tests=all.js --quitOnFinish=false --chromiumConsole=true",
      "test:packages": "npx electron . --user_settings_file=_data_test/user/user_settings.json --tests=installed_packages.js --chromiumConsole=false",
      "test:ui": "npx electron . --user_settings_file=_data_test/user/user_settings.json --tests=all.js --chromiumConsole=false",
      "test:fail": "npx electron . --user_settings_file=_data_test/user/user_settings.json --tests=fail.js --chromiumConsole=false",
      "test:tmp": "npx electron . --user_settings_file=_data_test/user/user_settings.json --tests=tmp.js --chromiumConsole=false",
      "test:tmpinv": "npx electron . --user_settings_file=_data_test/user/user_settings.json --tests=tmp.js --chromiumConsole=false",
      "test:jsx": "npx electron . --user_settings_file=_data_test/user/user_settings.json --tests=c:/typebox/app/packages_dev/typebox-test-jsx/test.js --chromiumConsole=true",
      "test:notes": "npx electron . --user_settings_file=_data_test/user/user_settings.json --tests=notes.js --chromiumConsole=true",
      "test:iohook": "npx electron . --user_settings_file=_data_test/user/user_settings.json --tests=iohook.js --chromiumConsole=false",
      "uglify": "taskr uglify"
   },
   "keywords": ["app", "desktop", "electron", "keystroke", "launcher", "productivity"],
