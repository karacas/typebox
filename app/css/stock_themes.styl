	--hoverColor: var(--darkColor); // hover //Btns/combos
	--textLowColor: #98a0a6; // name color
	// --textHighColor: #eeffff; // name color selected
	--mainSearchColor: var(--textHighColor);
	--hrTagColor: rgba(#fff, 0.125); // hr separador
	--caretColor: var(--accentColor);
	--buttonBackColor: var(--backColor); // backLightColor // Button Back
	--buttonFrontColor: #8ca1a7; // textLowColor// Button Front
	/* ExraColors */
	--warnColor: #d9b265;
	--errorColor: #f78c6c;
	--validColor: #4ec0a3;
	--asertColor: #9fdc7c;
	@extend $defaultCodeColors;
