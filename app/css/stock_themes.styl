body.experimental {
	// --padding: calc(12px * var(--scale));
	--backColor: linear-gradient(135deg, #3d486b 0%, #262f49 100%);
	--backLightColor: linear-gradient(95deg, rgba(#a691f9, 0.2) 0%, #394d89 100%);
	--backLightColor: linear-gradient(-100deg, rgba(#8e7dd2, 0.3) 0%, rgba(#394d89, 1) 100%);
	--darkColor: darken(#232b43, 2); // Dark
	--hoverColor: var(--darkColor); // hover //Btns/combos
	--textLowColor: #9b9dc9; // name color
	--textHighColor: #f9f5fa;
	--mainSearchColor: var(--textHighColor);
	--hrTagColor: rgba(#a691f9, 0.3); // hr separador
	--caretColor: #f9f5fa;
	--buttonBackColor: var(--backColor); // backLightColor // Button Back
	--buttonFrontColor: #8ca1a7; // textLowColor// Button Front
	--accentColor: #02c2e3;
	--accentColor2: #a691f9;
	--lowOpacity: 0.5;
	--medOpacity: 0.75;
	--hiOpacity: 0.9;
	@extend $forestCodeColors;
	--code-default: var(--textHighColor);
	--code-comment: var(--textLowColor);

	.ruleListCeldaWrapp {
		padding: 0px 8px !important;
	}

	.ruleListCelda {
		padding-left: 8px !important;
		padding-right: 8px !important;

		.main_ico {
			MX_transition(calc(0.2s * var(--animSpeedFactor)));
			transform: scale(0.85);
		}

		&.highLight {
			border-radius: 6px;
			box-shadow: 0px 6px 24px rgba(#000, 0.15), 0px 0px 36px rgba(#031140, 0.1) !important;
			background: var(--backLightColor) !important;

			.main_ico {
				animation: popAlt calc(0.2s * var(--animSpeedFactor)) ease-out;
				transform: scale(1);
			}
		}
	}

	#ruleViewerWrapp {
		padding: 12px !important;
		margin-left: 6px !important;
		width: calc(100% - 48px) !important;
		height: calc(100% - 24px) !important;
		background: linear-gradient(20deg, rgba(#8e7dd2, 0.2) 0%, rgba(#2f3f6f, 0.4) 100%);
		border: solid 1px rgba(#000, 0.15);
		border-radius: 8px;
	}

	.markdownView pre:not(.CodeMirror-line):not(.noBack) code, .webview pre:not(.CodeMirror-line):not(.noBack) code, #ruleViewer pre:not(.CodeMirror-line):not(.noBack) code, #ruleViewer pre:not(.CodeMirror-line):not(.CodeMirror-placeholder):not(.noBack) {
		background-color: #29324e !important;
	}
}

body.experimental2 {
