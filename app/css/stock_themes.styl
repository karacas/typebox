body.defaultBlack {
	--backColor: #15161c; // back
	--backLightColor: #1d1e24; // bar color
	// --accentColor: #4ec0a3; // HighLight /celeste
	// --accentColor2: #ffcd39; // HighLight /amarillo
	--darkColor: darken(#15161c, 5); // Dark // Dark
	// --textLowColor: #909fad; // name color
	// --textHighColor: #eeffff; // name color selected
	// --caretColor: var(--textHighColor); // HighLight /celeste
	--hrTagColor: rgba(255, 255, 255, 0.05); // hr separador
	--buttonBackColor: var(--backColor);
	--warnColor: #f28053;
	--errorColor: #fd5051;
	--validColor: #4ebe73;
	--asertColor: #4ebe73;
	@extend $defaultCodeColorsAlt;
	--code-default: var(--textHighColor);
	--code-comment: var(--textLowColor);
}

body.defaultGray {
	--backColor: #282c34; // back
	--backLightColor: lighten(#282c34, 3); // bar color
	// --accentColor: #4ec0a3; // HighLight /celeste
	// --accentColor2: #ffcd39; // HighLight /amarillo
